Cell[CellGroupData[{

Cell["Okun\[CloseCurlyQuote]s law", "Section",
 CellChangeTimes->{{3.695920729150281*^9, 3.695920739737104*^9}}],

Cell[TextData[{
 "note that the information equilibrium condition for the labor model says:\n\
\nP = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["dGDP", "dL"], "=", " ", 
    RowBox[{"k", " ", 
     FractionBox["GDP", "L"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nso \n\nP",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", " ", 
    RowBox[{"k", " ", 
     FractionBox["GDP", "L"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nor\n\nL",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", " ", 
    RowBox[{"k", " ", 
     FractionBox["GDP", "P"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nsuch that\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["d", "dt"], "log", " "}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "L",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", " ", 
    RowBox[{
     FractionBox["d", "dt"], "log", " ", 
     RowBox[{"(", 
      RowBox[{"k", " ", 
       FractionBox["GDP", "P"]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["d", "dt"], "log", " "}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "L",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", " ", 
    RowBox[{
     FractionBox["d", "dt"], "log", " ", 
     RowBox[{"(", 
      FractionBox["GDP", "P"], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "+ ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["d", "dt"], "log", " ", "k"}], TraditionalForm]]],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["d", "dt"], "log", " "}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "L",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", " ", 
    RowBox[{
     FractionBox["d", "dt"], "log", " ", 
     RowBox[{"(", 
      FractionBox["GDP", "P"], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "+ ",
 Cell[BoxData[
  FormBox["0", TraditionalForm]]],
 " \t... which is one form of Okun\[CloseCurlyQuote]s law (real GDP changes \
mean employment changes)"
}], "Text",
 CellChangeTimes->{{3.6959203498367605`*^9, 3.69592051721708*^9}, {
  3.6959205574705563`*^9, 3.695920595426428*^9}, {3.695920651492732*^9, 
